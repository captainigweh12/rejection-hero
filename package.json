    "eas:submit:ios:latest": "eas submit --platform ios --latest",
    "credentials:android": "eas credentials --platform android",
    "credentials:ios": "eas credentials --platform ios",
    "ci:install": "bun install || bun install --force",
    "ci:check": "npm run lint && npm run typecheck && npm run test:ci",
    "ci:build:android": "eas build --platform android --profile production --non-interactive",
    "ci:build:ios": "eas build --platform ios --profile production --non-interactive",
    "ci:build:android:preview": "eas build --platform android --profile preview --non-interactive",
    "ci:build:ios:preview": "eas build --platform ios --profile preview --non-interactive",
